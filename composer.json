{
  "name": "spryker/shopping-list-storage",
  "description": "ShoppingListStorage module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/company-user": "^1.0.0",
    "spryker/customer": "^7.0.0",
    "spryker/event-behavior": "^1.1.0",
    "spryker/kernel": "^3.17.0",
    "spryker/shopping-list": "^1.0.0",
    "spryker/key-builder": "*",
    "spryker/locale": "*",
    "spryker/storage": "*",
    "spryker/propel-orm": "^1.0.0",
    "spryker/shopping-list-session-extension": "*",
    "spryker/synchronization": "^0.2.0 || ^1.0.0",
    "spryker/synchronization-extension": "^1.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/event": "*",
    "spryker/testify": "*",
    "spryker/shopping-list-session": "*"
  },
  "suggest": {
    "spryker/event": "If you want to use Event plugins, minimum required version: 2.1.0",
    "spryker/shopping-list-session": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
